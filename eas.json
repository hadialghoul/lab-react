{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "autoIncrement": true,
        "buildConfiguration": "Release"
      },
      "android": {
        "autoIncrement": true,
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "Alii.jamal@hotmail.com",
        "ascAppId": "6754175139",
        "appleTeamId": "67UFRV9J5R"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-your-service-account-key.json",
        "track": "production"
      }
    }
  }
}
